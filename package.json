{
  "name": "redux-persist-transform-passwords",
  "version": "1.2.6",
  "description": "Store parts of your state tree in keytar",
  "main": "lib/index.js",
  "jest": {
    "testMatch": [
      "**/test/**"
    ]
  },
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "babel src --out-dir lib && npm run copy-types",
    "test": "jest",
    "clean": "rimraf lib",
    "copy-types": "cp -r type-definitions/ lib/",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CharlieHess/redux-persist-transform-passwords.git"
  },
  "homepage": "https://github.com/CharlieHess/redux-persist-transform-passwords",
  "author": "Charlie Hess",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "keytar": "^4.0.3",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.24.1",
    "deep-freeze": "0.0.1",
    "jest-cli": "^20.0.4",
    "rimraf": "~2.5.4"
  },
  "peerDependencies": {
    "redux-persist": ">=4.0.0"
  },
  "keywords": [
    "redux",
    "redux-persist",
    "redux-persist-transform",
    "keytar",
    "passwords",
    "transform-passwords"
  ]
}
